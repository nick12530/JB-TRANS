                    "content": "Index: \n===================================================================\n--- \n+++ \n@@ -1,1 +1,743 @@\n- \n\\ No newline at end of file\n+import React from 'react';\r\n+import { useTheme } from '../contexts/ThemeContext';\r\n+\r\n+interface Shop {\r\n+  id: number;\r\n+  name: string;\r\n+  address: string;\r\n+  county: string;\r\n+  location: string;\r\n+  coordinates: { lat: number; lng: number };\r\n+  phone: string;\r\n+  email: string;\r\n+  workingHours: string;\r\n+  description: string;\r\n+  shopImage: string;\r\n+  assignedBarbers: number[];\r\n+  isActive: boolean;\r\n+  createdAt: string;\r\n+}\r\n+\r\n+export default function ShopManagement() {\r\n+  const { isDarkMode, colors } = useTheme();\r\n+  \r\n+  const [shops, setShops] = React.useState<Shop[]>([\r\n+    {\r\n+      id: 1,\r\n+      name: \"Elite Barbershop\",\r\n+      address: \"123 Kenyatta Avenue, Nairobi CBD\",\r\n+      county: \"Nairobi\",\r\n+      location: \"Nairobi CBD\",\r\n+      coordinates: { lat: -1.2921, lng: 36.8219 },\r\n+      phone: \"+254 700 123 456\",\r\n+      email: \"info@elitebarbershop.com\",\r\n+      workingHours: \"Mon-Sat: 9AM-6PM\",\r\n+      description: \"Premium barbershop offering classic cuts and modern styling\",\r\n+      shopImage: \"https://images.unsplash.com/photo-1560066984-138dadb4c035?w=400&h=250&fit=crop\",\r\n+      assignedBarbers: [1, 2],\r\n+      isActive: true,\r\n+      createdAt: \"2024-01-01\"\r\n+    },\r\n+    {\r\n+      id: 2,\r\n+      name: \"Style Studio\",\r\n+      address: \"456 Westlands Road, Westlands\",\r\n+      county: \"Nairobi\",\r\n+      location: \"Westlands\",\r\n+      coordinates: { lat: -1.2654, lng: 36.8021 },\r\n+      phone: \"+254 700 234 567\",\r\n+      email: \"info@stylestudio.com\",\r\n+      workingHours: \"Mon-Sat: 8AM-7PM\",\r\n+      description: \"Modern hair studio with contemporary styling services\",\r\n+      shopImage: \"https://images.unsplash.com/photo-1503951914875-452162b0f3d1?w=400&h=250&fit=crop\",\r\n+      assignedBarbers: [3, 4],\r\n+      isActive: true,\r\n+      createdAt: \"2024-01-02\"\r\n+    },\r\n+    {\r\n+      id: 3,\r\n+      name: \"Gentleman's Cut\",\r\n+      address: \"789 Karen Road, Karen\",\r\n+      county: \"Nairobi\",\r\n+      location: \"Karen\",\r\n+      coordinates: { lat: -1.3197, lng: 36.8140 },\r\n+      phone: \"+254 700 345 678\",\r\n+      email: \"info@gentlemanscut.com\",\r\n+      workingHours: \"Mon-Sat: 9AM-5PM\",\r\n+      description: \"Traditional barbershop with classic grooming services\",\r\n+      shopImage: \"https://images.unsplash.com/photo-1585747860715-2ba37e788b70?w=400&h=250&fit=crop\",\r\n+      assignedBarbers: [5],\r\n+      isActive: true,\r\n+      createdAt: \"2024-01-03\"\r\n+    }\r\n+  ]);\r\n+\r\n+  const [barbers] = React.useState([\r\n+    { id: 1, name: \"John Smith\", specialty: \"Fade & Beard\" },\r\n+    { id: 2, name: \"Mike Jones\", specialty: \"Classic Cuts\" },\r\n+    { id: 3, name: \"Sarah Wilson\", specialty: \"Hair Styling\" },\r\n+    { id: 4, name: \"Alex Brown\", specialty: \"Modern Cuts\" },\r\n+    { id: 5, name: \"David Lee\", specialty: \"Beard Grooming\" }\r\n+  ]);\r\n+\r\n+  const [showAddForm, setShowAddForm] = React.useState(false);\r\n+  const [editingShop, setEditingShop] = React.useState<Shop | null>(null);\r\n+  const [newShop, setNewShop] = React.useState({\r\n+    name: '',\r\n+    address: '',\r\n+    county: '',\r\n+    location: '',\r\n+    phone: '',\r\n+    email: '',\r\n+    workingHours: '',\r\n+    description: '',\r\n+    shopImage: '',\r\n+    assignedBarbers: [] as number[]\r\n+  });\r\n+\r\n+  const [viewMode, setViewMode] = React.useState<'list' | 'map'>('list');\r\n+\r\n+  const handleAddShop = () => {\r\n+    const shop: Shop = {\r\n+      id: Math.max(...shops.map(s => s.id)) + 1,\r\n+      ...newShop,\r\n+      coordinates: { lat: -1.2921, lng: 36.8219 }, // Default Nairobi coordinates\r\n+      isActive: true,\r\n+      createdAt: new Date().toISOString().split('T')[0]\r\n+    };\r\n+    \r\n+    setShops([...shops, shop]);\r\n+    setNewShop({\r\n+      name: '',\r\n+      address: '',\r\n+      county: '',\r\n+      location: '',\r\n+      phone: '',\r\n+      email: '',\r\n+      workingHours: '',\r\n+      description: '',\r\n+      shopImage: '',\r\n+      assignedBarbers: []\r\n+    });\r\n+    setShowAddForm(false);\r\n+  };\r\n+\r\n+  const handleEditShop = (shop: Shop) => {\r\n+    setEditingShop(shop);\r\n+    setNewShop({\r\n+      name: shop.name,\r\n+      address: shop.address,\r\n+      county: shop.county,\r\n+      location: shop.location,\r\n+      phone: shop.phone,\r\n+      email: shop.email,\r\n+      workingHours: shop.workingHours,\r\n+      description: shop.description,\r\n+      shopImage: shop.shopImage,\r\n+      assignedBarbers: shop.assignedBarbers\r\n+    });\r\n+    setShowAddForm(true);\r\n+  };\r\n+\r\n+  const handleUpdateShop = () => {\r\n+    if (!editingShop) return;\r\n+    \r\n+    setShops(shops.map(shop => \r\n+      shop.id === editingShop.id \r\n+        ? { ...shop, ...newShop }\r\n+        : shop\r\n+    ));\r\n+    \r\n+    setEditingShop(null);\r\n+    setNewShop({\r\n+      name: '',\r\n+      address: '',\r\n+      county: '',\r\n+      location: '',\r\n+      phone: '',\r\n+      email: '',\r\n+      workingHours: '',\r\n+      description: '',\r\n+      shopImage: '',\r\n+      assignedBarbers: []\r\n+    });\r\n+    setShowAddForm(false);\r\n+  };\r\n+\r\n+  const handleDeleteShop = (shopId: number) => {\r\n+    if (window.confirm('Are you sure you want to delete this shop?')) {\r\n+      setShops(shops.filter(shop => shop.id !== shopId));\r\n+    }\r\n+  };\r\n+\r\n+  const toggleShopStatus = (shopId: number) => {\r\n+    setShops(shops.map(shop => \r\n+      shop.id === shopId \r\n+        ? { ...shop, isActive: !shop.isActive }\r\n+        : shop\r\n+    ));\r\n+  };\r\n+\r\n+  return (\r\n+    <div style={{ \r\n+      padding: '20px', \r\n+      backgroundColor: colors.background, \r\n+      minHeight: '100vh',\r\n+      fontFamily: '\"Lora\", serif'\r\n+    }}>\r\n+      <div style={{ maxWidth: '1200px', margin: '0 auto' }}>\r\n+        {/* Header */}\r\n+        <div style={{ \r\n+          display: 'flex', \r\n+          justifyContent: 'space-between', \r\n+          alignItems: 'center',\r\n+          marginBottom: '30px'\r\n+        }}>\r\n+          <div>\r\n+            <h1 style={{ \r\n+              color: colors.textHeading, \r\n+              margin: '0 0 8px 0',\r\n+              fontFamily: '\"Montserrat\", sans-serif',\r\n+              fontSize: '2rem',\r\n+              fontWeight: '700'\r\n+            }}>\r\n+              Shop Management\r\n+            </h1>\r\n+            <p style={{ \r\n+              color: colors.textBody, \r\n+              margin: '0',\r\n+              fontSize: '1rem'\r\n+            }}>\r\n+              Manage barber shops, locations, and assigned barbers\r\n+            </p>\r\n+          </div>\r\n+          \r\n+          <div style={{ display: 'flex', gap: '12px', alignItems: 'center' }}>\r\n+            <div style={{ display: 'flex', gap: '8px' }}>\r\n+              <button\r\n+                onClick={() => setViewMode('list')}\r\n+                style={{\r\n+                  padding: '8px 16px',\r\n+                  border: 'none',\r\n+                  borderRadius: '6px',\r\n+                  backgroundColor: viewMode === 'list' ? '#17A589' : colors.surface,\r\n+                  color: viewMode === 'list' ? '#FFFFFF' : colors.textBody,\r\n+                  cursor: 'pointer',\r\n+                  fontSize: '14px',\r\n+                  fontWeight: '500',\r\n+                  fontFamily: '\"Montserrat\", sans-serif'\r\n+                }}\r\n+              >\r\n+                üìã List\r\n+              </button>\r\n+              <button\r\n+                onClick={() => setViewMode('map')}\r\n+                style={{\r\n+                  padding: '8px 16px',\r\n+                  border: 'none',\r\n+                  borderRadius: '6px',\r\n+                  backgroundColor: viewMode === 'map' ? '#17A589' : colors.surface,\r\n+                  color: viewMode === 'map' ? '#FFFFFF' : colors.textBody,\r\n+                  cursor: 'pointer',\r\n+                  fontSize: '14px',\r\n+                  fontWeight: '500',\r\n+                  fontFamily: '\"Montserrat\", sans-serif'\r\n+                }}\r\n+              >\r\n+                üó∫Ô∏è Map\r\n+              </button>\r\n+            </div>\r\n+            \r\n+            <button\r\n+              onClick={() => setShowAddForm(true)}\r\n+              style={{\r\n+                backgroundColor: '#17A589',\r\n+                color: '#FFFFFF',\r\n+                padding: '12px 24px',\r\n+                borderRadius: '8px',\r\n+                border: 'none',\r\n+                cursor: 'pointer',\r\n+                fontSize: '14px',\r\n+                fontWeight: '600',\r\n+                fontFamily: '\"Montserrat\", sans-serif',\r\n+                display: 'flex',\r\n+                alignItems: 'center',\r\n+                gap: '8px',\r\n+                boxShadow: '0 2px 4px rgba(23, 165, 137, 0.3)',\r\n+                transition: 'all 0.2s ease'\r\n+              }}\r\n+              onMouseEnter={(e) => {\r\n+                e.currentTarget.style.backgroundColor = '#138D75';\r\n+                e.currentTarget.style.transform = 'translateY(-1px)';\r\n+                e.currentTarget.style.boxShadow = '0 4px 8px rgba(23, 165, 137, 0.4)';\r\n+              }}\r\n+              onMouseLeave={(e) => {\r\n+                e.currentTarget.style.backgroundColor = '#17A589';\r\n+                e.currentTarget.style.transform = 'translateY(0)';\r\n+                e.currentTarget.style.boxShadow = '0 2px 4px rgba(23, 165, 137, 0.3)';\r\n+              }}\r\n+            >\r\n+              ‚ûï Add Shop\r\n+            </button>\r\n+          </div>\r\n+        </div>\r\n+\r\n+        {/* Add/Edit Form */}\r\n+        {showAddForm && (\r\n+          <div style={{\r\n+            backgroundColor: colors.surface,\r\n+            padding: '24px',\r\n+            borderRadius: '12px',\r\n+            marginBottom: '24px',\r\n+            border: `1px solid ${colors.border}`,\r\n+            boxShadow: '0 4px 12px rgba(0,0,0,0.1)'\r\n+          }}>\r\n+            <h3 style={{ \r\n+              color: colors.textHeading, \r\n+              marginBottom: '20px',\r\n+              fontFamily: '\"Montserrat\", sans-serif',\r\n+              fontSize: '1.25rem'\r\n+            }}>\r\n+              {editingShop ? 'Edit Shop' : 'Add New Shop'}\r\n+            </h3>\r\n+            \r\n+            <div style={{ \r\n+              display: 'grid', \r\n+              gridTemplateColumns: 'repeat(auto-fit, minmax(250px, 1fr))', \r\n+              gap: '16px',\r\n+              marginBottom: '20px'\r\n+            }}>\r\n+              <input\r\n+                type=\"text\"\r\n+                placeholder=\"Shop Name\"\r\n+                value={newShop.name}\r\n+                onChange={(e) => setNewShop({...newShop, name: e.target.value})}\r\n+                style={{\r\n+                  padding: '12px',\r\n+                  border: `1px solid ${colors.border}`,\r\n+                  borderRadius: '8px',\r\n+                  backgroundColor: colors.inputBackground,\r\n+                  color: colors.textHeading,\r\n+                  fontSize: '14px',\r\n+                  fontFamily: '\"Lora\", serif'\r\n+                }}\r\n+              />\r\n+              <input\r\n+                type=\"text\"\r\n+                placeholder=\"Address\"\r\n+                value={newShop.address}\r\n+                onChange={(e) => setNewShop({...newShop, address: e.target.value})}\r\n+                style={{\r\n+                  padding: '12px',\r\n+                  border: `1px solid ${colors.border}`,\r\n+                  borderRadius: '8px',\r\n+                  backgroundColor: colors.inputBackground,\r\n+                  color: colors.textHeading,\r\n+                  fontSize: '14px',\r\n+                  fontFamily: '\"Lora\", serif'\r\n+                }}\r\n+              />\r\n+              <select\r\n+                value={newShop.county}\r\n+                onChange={(e) => setNewShop({...newShop, county: e.target.value})}\r\n+                style={{\r\n+                  padding: '12px',\r\n+                  border: `1px solid ${colors.border}`,\r\n+                  borderRadius: '8px',\r\n+                  backgroundColor: colors.inputBackground,\r\n+                  color: colors.textHeading,\r\n+                  fontSize: '14px',\r\n+                  fontFamily: '\"Lora\", serif'\r\n+                }}\r\n+              >\r\n+                <option value=\"\">Select County</option>\r\n+                <option value=\"Nairobi\">Nairobi</option>\r\n+                <option value=\"Kiambu\">Kiambu</option>\r\n+              </select>\r\n+              <input\r\n+                type=\"text\"\r\n+                placeholder=\"Location\"\r\n+                value={newShop.location}\r\n+                onChange={(e) => setNewShop({...newShop, location: e.target.value})}\r\n+                style={{\r\n+                  padding: '12px',\r\n+                  border: `1px solid ${colors.border}`,\r\n+                  borderRadius: '8px',\r\n+                  backgroundColor: colors.inputBackground,\r\n+                  color: colors.textHeading,\r\n+                  fontSize: '14px',\r\n+                  fontFamily: '\"Lora\", serif'\r\n+                }}\r\n+              />\r\n+              <input\r\n+                type=\"tel\"\r\n+                placeholder=\"Phone\"\r\n+                value={newShop.phone}\r\n+                onChange={(e) => setNewShop({...newShop, phone: e.target.value})}\r\n+                style={{\r\n+                  padding: '12px',\r\n+                  border: `1px solid ${colors.border}`,\r\n+                  borderRadius: '8px',\r\n+                  backgroundColor: colors.inputBackground,\r\n+                  color: colors.textHeading,\r\n+                  fontSize: '14px',\r\n+                  fontFamily: '\"Lora\", serif'\r\n+                }}\r\n+              />\r\n+              <input\r\n+                type=\"email\"\r\n+                placeholder=\"Email\"\r\n+                value={newShop.email}\r\n+                onChange={(e) => setNewShop({...newShop, email: e.target.value})}\r\n+                style={{\r\n+                  padding: '12px',\r\n+                  border: `1px solid ${colors.border}`,\r\n+                  borderRadius: '8px',\r\n+                  backgroundColor: colors.inputBackground,\r\n+                  color: colors.textHeading,\r\n+                  fontSize: '14px',\r\n+                  fontFamily: '\"Lora\", serif'\r\n+                }}\r\n+              />\r\n+              <input\r\n+                type=\"text\"\r\n+                placeholder=\"Working Hours\"\r\n+                value={newShop.workingHours}\r\n+                onChange={(e) => setNewShop({...newShop, workingHours: e.target.value})}\r\n+                style={{\r\n+                  padding: '12px',\r\n+                  border: `1px solid ${colors.border}`,\r\n+                  borderRadius: '8px',\r\n+                  backgroundColor: colors.inputBackground,\r\n+                  color: colors.textHeading,\r\n+                  fontSize: '14px',\r\n+                  fontFamily: '\"Lora\", serif'\r\n+                }}\r\n+              />\r\n+              <input\r\n+                type=\"url\"\r\n+                placeholder=\"Shop Image URL\"\r\n+                value={newShop.shopImage}\r\n+                onChange={(e) => setNewShop({...newShop, shopImage: e.target.value})}\r\n+                style={{\r\n+                  padding: '12px',\r\n+                  border: `1px solid ${colors.border}`,\r\n+                  borderRadius: '8px',\r\n+                  backgroundColor: colors.inputBackground,\r\n+                  color: colors.textHeading,\r\n+                  fontSize: '14px',\r\n+                  fontFamily: '\"Lora\", serif'\r\n+                }}\r\n+              />\r\n+            </div>\r\n+            \r\n+            <textarea\r\n+              placeholder=\"Description\"\r\n+              value={newShop.description}\r\n+              onChange={(e) => setNewShop({...newShop, description: e.target.value})}\r\n+              rows={3}\r\n+              style={{\r\n+                width: '100%',\r\n+                padding: '12px',\r\n+                border: `1px solid ${colors.border}`,\r\n+                borderRadius: '8px',\r\n+                backgroundColor: colors.inputBackground,\r\n+                color: colors.textHeading,\r\n+                fontSize: '14px',\r\n+                marginBottom: '20px',\r\n+                resize: 'vertical',\r\n+                fontFamily: '\"Lora\", serif'\r\n+              }}\r\n+            />\r\n+            \r\n+            <div style={{ display: 'flex', gap: '12px', justifyContent: 'flex-end' }}>\r\n+              <button\r\n+                onClick={() => {\r\n+                  setShowAddForm(false);\r\n+                  setEditingShop(null);\r\n+                  setNewShop({\r\n+                    name: '',\r\n+                    address: '',\r\n+                    county: '',\r\n+                    location: '',\r\n+                    phone: '',\r\n+                    email: '',\r\n+                    workingHours: '',\r\n+                    description: '',\r\n+                    shopImage: '',\r\n+                    assignedBarbers: []\r\n+                  });\r\n+                }}\r\n+                style={{\r\n+                  padding: '